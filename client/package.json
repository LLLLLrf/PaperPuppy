{
  "name": "PaperPuppy",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron:dev": "vite --mode electron",
    "electron:build": "vite build --mode electron && electron-builder"
  },
  "build": {
    "appId": "com.paperpuppy.app",
    "productName": "PaperPuppy",
    "directories": {
      "output": "dist_electron"
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "src/assets/logo_white.jpg"
    ],
    "icon": "src/assets/logo_white.jpg",
    "mac": {
      "target": "dmg",
      "icon": "src/assets/logo_white.jpg"
    },
    "win": {
      "target": "nsis",
      "icon": "src/assets/logo_white.jpg"
    },
    "linux": {
      "target": "AppImage",
      "icon": "src/assets/logo_white.jpg"
    }
  },
  "dependencies": {
    "d3": "^7.9.0",
    "html2canvas": "^1.4.1",
    "html2pdf.js": "^0.12.1",
    "jspdf": "^3.0.4",
    "jspdf-autotable": "^5.0.2",
    "jspdf-fonts": "^0.0.2",
    "marked": "^17.0.1",
    "vue": "^3.5.24",
    "wordcloud": "^1.2.3"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "electron": "^30.0.0",
    "electron-builder": "^26.0.12",
    "esbuild": "^0.25.12",
    "terser": "^5.44.1",
    "vite": "^5.0.0",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6"
  }
}
